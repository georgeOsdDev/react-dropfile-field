{
  "name": "react-dropfile-field",
  "version": "0.0.4",
  "description": "React component which handle file drop and preview",
  "main": "index.js",
  "scripts": {
    "browser": "browser-sync start --files example/* --server example",
    "watch:example": "watchify example/app.js -dv -o example/bundle.js",
    "start:example": "npm run watch:example & npm run browser",
    "test:local": "karma start",
    "test": "./node_modules/.bin/karma start --browsers Firefox --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/georgeosddev/react-dropfile-field"
  },
  "keywords": [
    "react",
    "react-component",
    "dragdrop"
  ],
  "author": "Takeharu.Oshida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgeosddev/react-dropfile-field/issues"
  },
  "homepage": "https://github.com/georgeosddev/react-dropfile-field",
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "babelify": "7.3.0",
    "browser-sync": "2.7.12",
    "chai": "3.5.0",
    "eslint": "2.9.0",
    "eslint-plugin-react": "5.0.1",
    "karma": "0.13.22",
    "karma-browserify": "5.0.4",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-cli": "1.0.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-mocha": "1.0.1",
    "karma-safari-launcher": "1.0.0",
    "karma-spec-reporter": "0.0.26",
    "material-ui": "0.14.4",
    "mocha": "2.2.5",
    "react-addons-test-utils": "15.0.2",
    "react-tap-event-plugin": "1.0.0",
    "sinon": "1.15.1",
    "watchify": "3.2.2"
  },
  "dependencies": {
    "react": "15.0.2",
    "immutable": "3.7.4"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "compact": false,
          "presets": [
            "es2015",
            "react",
            "stage-2"
          ]
        }
      ]
    ]
  }
}
