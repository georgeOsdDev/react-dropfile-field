{
  "name": "react-dropfile-field",
  "version": "0.0.1",
  "description": "React component which handle file drop and preview",
  "main": "index.js",
  "scripts": {
    "browser": "browser-sync start --files example/* --server example",
    "watch:example": "watchify example/app.js -dv -o example/bundle.js",
    "start:example": "npm run watch:example & npm run browser",
    "test:local": "karma start",
    "test": "./node_modules/.bin/karma start --browsers Firefox --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/georgeosddev/react-dropfile-field"
  },
  "keywords": [
    "react",
    "react-component",
    "dragdrop"
  ],
  "author": "Takeharu.Oshida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgeosddev/react-dropfile-field/issues"
  },
  "homepage": "https://github.com/georgeosddev/react-dropfile-field",
  "devDependencies": {
    "chai": "^2.3.0",
    "karma": "^0.12.35",
    "karma-browserify": "^4.2.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.12",
    "karma-cli": "0.0.4",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.1.10",
    "karma-safari-launcher": "^0.1.1",
    "karma-spec-reporter": "0.0.19",
    "mocha": "^2.2.5",
    "sinon": "^1.15.1",
    "react-tap-event-plugin": "^0.1.7",
    "material-ui": "^0.9.2",
    "eslint-plugin-react": "^2.5.2",
    "babelify": "^6.1.2",
    "babel-eslint": "^3.1.17",
    "eslint": "^0.23.0",
    "watchify": "^3.2.2",
    "browser-sync": "^2.7.12"
  },
  "dependencies": {
    "react": "^0.13.3",
    "immutable": "^3.7.4"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "compact": false
        }
      ]
    ]
  }
}
